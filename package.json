{
  "name": "kino-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "prepare": "husky install",
    "dev": "next dev --turbopack",
