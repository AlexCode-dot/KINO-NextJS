  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "formdata-node": "^6.0.3",
    "mongoose": "^8.14.1",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^29.7.0",
    "@testing-library/react": "^16.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.5.3",
    "sass": "^1.87.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css,scss,md,json,yml,yaml}": "prettier --write"
